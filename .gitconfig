[user]
	name = Steven Harman
	email = steveharman@gmail.com
[color]
	branch = auto
	diff = auto
	status = auto
[color "branch"]
  current = yellow reverse
  local = yellow
  remote = green
[color "diff"]
  meta = yellow bold
  frag = magenta bold
  old = red bold
  new = green bold
[color "status"]
  added = yellow
  changed = green
  untracked = cyan
[merge]
	tool = opendiff
[core]
	autocrlf = input
	legacyheaders = false
[repack]
	usedeltabaseoffset = true
[alias]
	ci = commit
	co = checkout
	st = status
  ch = cherry -v origin
  mine = log --author=Steven
[github]
	user = stevenharman
	token = [GITHUB TOKEN HERE]
